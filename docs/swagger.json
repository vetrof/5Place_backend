{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "API для приложения 5Place - поиск и управление местами",
        "title": "5Place API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:5555",
    "basePath": "/",
    "paths": {
        "/place/cities/country/{country_id}": {
            "get": {
                "description": "Возвращает список городов для указанной страны",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "cities"
                ],
                "summary": "Получить города по стране",
                "parameters": [
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "ID страны",
                        "name": "country_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.ResponseGeneric-array_models_City-handlers_ResponseMeta"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/place/countries": {
            "get": {
                "description": "Возвращает список всех доступных стран",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "countries"
                ],
                "summary": "Получить список стран",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.ResponseGeneric-array_models_Country-handlers_ResponseMeta"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/place/places/city/{city_id}": {
            "get": {
                "description": "Возвращает список всех мест в указанном городе",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "places"
                ],
                "summary": "Получить места в городе",
                "parameters": [
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "ID города",
                        "name": "city_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.ResponseGeneric-array_models_Place-handlers_ResponseMeta"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/place/places/near": {
            "get": {
                "description": "Возвращает места в указанном радиусе от координат",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "places"
                ],
                "summary": "Найти места поблизости",
                "parameters": [
                    {
                        "type": "number",
                        "example": 43.222,
                        "description": "Широта",
                        "name": "lat",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "number",
                        "example": 76.8512,
                        "description": "Долгота",
                        "name": "long",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "default": 10,
                        "example": 10,
                        "description": "Максимальное количество результатов (1-100)",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "default": 5000,
                        "example": 5000,
                        "description": "Радиус поиска в метрах (1-50000)",
                        "name": "radius",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.ResponseGeneric-array_models_Place-handlers_ResponseMeta"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/place/places/random": {
            "get": {
                "description": "Возвращает случайные места с возможностью фильтрации по стране или городу",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "places"
                ],
                "summary": "Получить случайные места",
                "parameters": [
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "ID страны для фильтрации",
                        "name": "country",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "ID города для фильтрации",
                        "name": "city",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.ResponseGeneric-array_models_Place-handlers_ResponseMeta"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/place/places/{place_id}": {
            "get": {
                "description": "Возвращает подробную информацию о месте по его ID",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "places"
                ],
                "summary": "Получить детали места",
                "parameters": [
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "ID места",
                        "name": "place_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.ResponseGeneric-array_models_Place-handlers_ResponseMeta"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/handlers.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "handlers.Coordinates": {
            "type": "object",
            "properties": {
                "lat": {
                    "type": "number"
                },
                "lon": {
                    "type": "number"
                }
            }
        },
        "handlers.ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string",
                    "example": "Invalid parameter"
                }
            }
        },
        "handlers.ResponseGeneric-array_models_City-handlers_ResponseMeta": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.City"
                    }
                },
                "meta": {
                    "$ref": "#/definitions/handlers.ResponseMeta"
                }
            }
        },
        "handlers.ResponseGeneric-array_models_Country-handlers_ResponseMeta": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Country"
                    }
                },
                "meta": {
                    "$ref": "#/definitions/handlers.ResponseMeta"
                }
            }
        },
        "handlers.ResponseGeneric-array_models_Place-handlers_ResponseMeta": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Place"
                    }
                },
                "meta": {
                    "$ref": "#/definitions/handlers.ResponseMeta"
                }
            }
        },
        "handlers.ResponseMeta": {
            "type": "object",
            "properties": {
                "center": {
                    "$ref": "#/definitions/handlers.Coordinates"
                },
                "count": {
                    "type": "integer"
                },
                "limit": {
                    "type": "integer"
                },
                "searchRadius": {
                    "type": "number"
                }
            }
        },
        "models.City": {
            "type": "object",
            "properties": {
                "country": {
                    "type": "string",
                    "example": "Kazakhstan"
                },
                "geom": {
                    "type": "string",
                    "example": "POINT(76.8512 43.2220)"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "name": {
                    "type": "string",
                    "example": "Almaty"
                },
                "points": {
                    "type": "integer",
                    "example": 150
                }
            }
        },
        "models.Country": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "name": {
                    "type": "string",
                    "example": "Kazakhstan"
                }
            }
        },
        "models.Place": {
            "type": "object",
            "properties": {
                "cityName": {
                    "type": "string",
                    "example": "Almaty"
                },
                "desc": {
                    "type": "string",
                    "example": "Famous hill with panoramic view of Almaty city"
                },
                "distance": {
                    "type": "number",
                    "example": 1245.67
                },
                "geom": {
                    "type": "string",
                    "example": "POINT(76.9572 43.2316)"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "name": {
                    "type": "string",
                    "example": "Kok-Tobe Hill"
                },
                "photos": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "photo1.jpg",
                        "photo2.jpg"
                    ]
                }
            }
        }
    },
    "tags": [
        {
            "description": "Операции со странами",
            "name": "countries"
        },
        {
            "description": "Операции с городами",
            "name": "cities"
        },
        {
            "description": "Операции с местами и достопримечательностями",
            "name": "places"
        }
    ]
}